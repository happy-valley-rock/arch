[user]
	name = _
	email = _
[color]
	ui = auto
[core]
	editor = nvim
[alias]
	st = status
	co = checkout
	br = branch
  cm = commit -m
	last = log -1 HEAD
	rv = reset --soft HEAD~1
  lg = log --oneline --graph --decorate --all
  sh = push
  shbr = push --set-upstream origin $(git rev-parse --abbrev-ref HEAD | grep HEAD)
